@model List<DogModel>

@if(Model == null || !Model.Any())
{
    <h1>Could not find dogs</h1>
}
else
{
    <h1>Our dogs</h1>

    @foreach(DogModel dog in Model)
    {
        <div class="dog-item">
            <h2>@dog.Name</h2> <!-- Block element -->
            <label>Favorite food:</label> <!-- Inline element -->
            <span>@dog.FavFood</span> <!-- Inline element --> 
        </div>
    }
}
